<div class="nav2" id="navbar2">
  <div class="col-md-10 mr-2 ml-2" id="locationField">
    <input id="autocomplete" type="text" placeholder="Enter Location" onFocus="geolocate()"></input>
  </div>
  <a href='/mapPage' <button type="submit" id="searchBtn" class="btn btn-danger">Search</button>
</div>
</div>

<table id="address">
  <tr>
    <td class="label" type="hidden">Street address</td>
    <td class="slimField"><input class="field" type="hidden" id="street_number" disabled="true"></input></td>
    <td class="wideField" colspan="2"><input class="field" id="route" disabled="true" type="hidden"></input></td>
  </tr>
  <tr>
    <td class="label">City</td>
    <td class="wideField" colspan="3"><input class="field" id="locality" disabled="true" type="hidden"></input></td>
  </tr>
  <tr>
    <td class="label">State</td>
    <td class="slimField"><input class="field" id="administrative_area_level_1" disabled="true" type="hidden"></input></td>
    <td class="label">Zip code</td>
    <td class="wideField"><input class="field" id="postal_code" disabled="true" type="hidden"></input></td>
  </tr>
  <tr>
    <td class="label">Country</td>
    <td class="wideField" colspan="3"><input class="field" id="country" disabled="true" type="hidden"></input></td>
  </tr>
</table>
