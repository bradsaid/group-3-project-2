<div id="addbano">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h2>Add A Bathroom!</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Bathroom Name:</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter name of location">
          </div>
          <div class="form-group">
            <label for="example-description">Cleanliness (1-5)</label>
            <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter cleanliness rating"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>Bathrooms:</h3>
        <ul id="example-list" class="list-group">
          {{#each potties}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.name}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
<script src="/js/index.js"></script>



