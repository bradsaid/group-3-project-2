<div id="addbano">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h2>Add A Restroom!</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Restroom Name:</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter Restroom Name">
          </div>
          <div class="form-group">
            <label for="example-description">Cleanliness Rating (1-5)</label>
            <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter Cleanliness Rating"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>Restrooms:</h3>
        <ul id="example-list" class="list-group">
          {{#each potties}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.name}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>


<script src="/js/index.js"></script>

